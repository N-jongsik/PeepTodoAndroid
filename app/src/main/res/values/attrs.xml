<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundedLayout">
        <attr name="roundedLayout_borderColor" format="color" /> <!-- 테두리 색상 -->
        <attr name="roundedLayout_backgroundColor" format="color" /> <!-- 배경 색상 -->
    </declare-styleable>

    <declare-styleable name="IconButtonTextView">
        <attr name="iconButton_borderColor" format="color" />
        <attr name="iconButton_backgroundColor" format="color" />
        <attr name="iconButton_iconSrc" format="reference" />
        <attr name="iconButton_iconTint" format="color" />
        <attr name="iconButton_text" format="string" />
        <attr name="iconButton_textTint" format="color" />
    </declare-styleable>

    <declare-styleable name="SmallIconButtonTextView">
        <attr name="SmalliconButton_borderColor" format="color" />
        <attr name="SmalliconButton_backgroundColor" format="color" />
        <attr name="SmalliconButton_iconSrc" format="reference" />
        <attr name="SmalliconButton_iconTint" format="color" />
        <attr name="SmalliconButton_text" format="string" />
        <attr name="SmalliconButton_textTint" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomCheckBox">
        <attr name="checkbox_iconColor" format="color"/>
        <attr name="checkbox_isChecked" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomAddSquare">
        <attr name="addSquare_color" format="color"/>
    </declare-styleable>

</resources>